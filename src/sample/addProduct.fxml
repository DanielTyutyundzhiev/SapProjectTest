<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="511.0" prefWidth="564.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddProductController">
    <children>
        <AnchorPane prefHeight="105.0" prefWidth="600.0" style="-fx-background-color: F88C75;">
            <children>
                <Label alignment="CENTER" layoutX="193.0" layoutY="39.0" prefHeight="17.0" prefWidth="214.0" text="Add a Product" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
            </children></AnchorPane>
        <Label layoutX="191.0" layoutY="124.0" prefHeight="17.0" prefWidth="124.0" text="Enter product name:" />
        <Label layoutX="185.0" layoutY="178.0" prefHeight="17.0" prefWidth="124.0" text="Enter product type:" />
        <Label layoutX="185.0" layoutY="227.0" text="Enter product brand:" />
        <Label layoutX="188.0" layoutY="268.0" text="Enter product price:" />
        <Label layoutX="185.0" layoutY="310.0" text="Enter product quantity:" />
        <TextField fx:id="prnameField" layoutX="186.0" layoutY="153.0" prefHeight="25.0" prefWidth="259.0" promptText="Product name" />
        <TextField fx:id="prtypeField" layoutX="186.0" layoutY="202.0" prefHeight="25.0" prefWidth="259.0" promptText="Product type" />
        <TextField fx:id="prbrandField" layoutX="186.0" layoutY="243.0" prefHeight="25.0" prefWidth="259.0" promptText="Product brand" />
        <TextField fx:id="prpriceField" layoutX="186.0" layoutY="285.0" prefHeight="25.0" prefWidth="259.0" promptText="Product price" />
        <TextField fx:id="prquantityField" layoutX="186.0" layoutY="327.0" prefHeight="25.0" prefWidth="259.0" promptText="Product quantity" />
        <Label fx:id="messageLabel" layoutX="188.0" layoutY="105.0" prefHeight="17.0" prefWidth="346.0" />
        <Button layoutX="240.0" layoutY="419.0" mnemonicParsing="false" onAction="#addProduct" prefHeight="25.0" prefWidth="156.0" style="-fx-background-color: F88C75;" text="Add" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button fx:id="closeBtn" layoutX="240.0" layoutY="463.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="25.0" prefWidth="156.0" style="-fx-background-color: F88C75;" text="Close" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
    </children>
</AnchorPane>
