<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminMenuController">
    <children>
        <Button layoutX="32.0" layoutY="213.0" mnemonicParsing="false" onAction="#editSP" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Edit A Sales Representative" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <AnchorPane prefHeight="93.0" prefWidth="600.0" style="-fx-background-color: F88C75;">
            <children>
                <ImageView fitHeight="62.0" fitWidth="58.0" layoutX="72.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img_admin.png" />
                    </image>
                </ImageView>
                <Label layoutX="251.0" layoutY="33.0" text="Admin Menu" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Button fx:id="addSPButt" alignment="CENTER" layoutX="32.0" layoutY="115.0" mnemonicParsing="false" onAction="#addSP" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Add A Sales Representative" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button fx:id="deleteSPButton" layoutX="32.0" layoutY="165.0" mnemonicParsing="false" onAction="#deleteSP" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Remove A Sales Representative" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button fx:id="addProduct" layoutX="336.0" layoutY="115.0" mnemonicParsing="false" onAction="#addProduct" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Add Product" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button layoutX="336.0" layoutY="165.0" mnemonicParsing="false" onAction="#deleteProduct" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Remove Product" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button layoutX="336.0" layoutY="213.0" mnemonicParsing="false" onAction="#editProduct" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Edit Product" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button fx:id="logouButt" layoutX="398.0" layoutY="332.0" mnemonicParsing="false" onAction="#logoutOnAction" prefHeight="38.0" prefWidth="154.0" style="-fx-background-color: F88C75;" text="Logout" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button layoutX="32.0" layoutY="263.0" mnemonicParsing="false" onAction="#previewSales" prefHeight="38.0" prefWidth="231.0" style="-fx-background-color: F88C75;" text="Preview Sales" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
    </children>
</AnchorPane>
